{
  "name": "ddattee/tools",
  "description": "ZF2 ddattee Tools",
  "type": "library",
  "keywords": [
    "zf2"
  ],
  "homepage": "http://www.ddattee.fr/",
  "license": "Apache-2.0",
  "require": {
    "php": ">=5.5",
    "ddattee/libs": "dev-master",
    "neilime/zf2-assets-bundle": "3.*@stable"
  },
  "require-dev": {
    "phpunit/phpunit"        : "^4.0",
    "zendframework/zend-test": "^2.0",
    "phpmd/phpmd": "^2.6",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "authors": [
    {
      "name": "David Dattee",
      "email": "david.dattee@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Tools\\": "src"
    },
    "classmap": [
      "src/Tools/Module.php"
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.zendframework.com/"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  }
}